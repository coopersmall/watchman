{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "target": "es2020",
    "paths": {
      "@domains/*": [
        ".src/domains/*"
      ],
      "@infrastructure/*": [
        ".src/infrastructure/*"
      ]
    },
    "baseUrl": "."
  },
  "module": {
    "type": "commonjs"
  }
}