{
  "name": "@watchman/core",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/coopersmall/watchman.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": "^16.0.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.yarnpkg.com"
  },
  "scripts": {
    "build": "swc src -d dist",
    "clean": "rm -rf dist rm -rf coverage",
    "dev": "pnpm watch:build",
    "precommit": "pnpm test",
    "test": "jest --coverage",
    "watch:build": "swc src -w --out-dir dist"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "jest": "^29.1.2",
    "nock": "^13.2.9"
  },
  "dependencies": {
    "axios": "^1.1.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}
